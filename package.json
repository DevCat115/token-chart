{
  "name": "app.decode",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:client": "next dev",
    "dev:wss": "dotenv -e .env.local tsx watch ./server/websocket-server.ts --tsconfig tsconfig.server.json",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:wss\"",
    "dev:cron": "dotenv -e .env.local tsx watch ./cron/job.ts --tsconfig tsconfig.server.json",
    "kill": "concurrently -k",
    "dev:azure:client": "next start",
    "dev:azure:wss": "dotenv -e .env tsx ./server/websocket-server.ts --tsconfig tsconfig.server.json",
    "dev:azure:cron": "dotenv -e .env tsx ./cron/job.ts --tsconfig tsconfig.server.json",
    "dev:azure:api": "concurrently \"npm run dev:azure:wss\" \"npm run dev:azure:cron\"",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint '**/*.{js,jsx,ts,tsx}' --fix",
    "prettier": "npx prettier '**/*.{js,jsx,ts,tsx,html,css,json,md}' --write",
    "lint:prettier": "npm run lint && npm run prettier",
    "docker:rebuild": "docker compose stop && docker compose rm web -f && docker rmi decode-web -f && docker compose rm -v -f && docker compose up --build",
    "prepare": "husky install"
  },
